window.onload = function() {
  sound.play().fade(0.6, 0, 8000, function () {
      sound2.play().fade(0, 0.5, 0);
  });
  refreshWhitenoise();
  fadeOutWhitenoiseFadeInHeading();
};

// audio functions
var sound = new Howl({
  urls: ['<%= asset_path "whitenoise.mp3" %>'],
});

var sound2 = new Howl({
  urls: ['<%= asset_path "Interstellar.mp3" %>'],
  loop: true,
});
